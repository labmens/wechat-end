  "version": "0.2.0",
  "configurations": [

    {
      "cwd": "${workspaceRoot}",
      "env": {
          "NODE_ENV": "development"
      },
      "type": "node",
      "sourceMaps": true,
      "runtimeExecutable": null,
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceRoot}/src/server.ts",
      "outFiles": [
        "${workspaceRoot}/dist/**/*.js"
      ]
    }, {
        "name": "Attach",
        "type": "node",
        "request": "attach",
        "port": 5858
      }
  ]
}